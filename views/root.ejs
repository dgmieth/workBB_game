<%- include('metaData/beginning.ejs', {title: info.pageTitle, nome: nome, imgAddress: imgAddress}) %>
<%- include('partials/header.ejs', {info: info}) %>
<style>
    button:not(.not-include){
        background-color: transparent;
        border:rgba(235, 119, 12, 1) .5px solid;
        color:rgba(235, 119, 12, 1);
        font-size:.95rem;
        width: 95%;
        margin: 6px 0;
    }
    button:hover:not(.btnActive, .not-include){
        font-weight: 600;
        border-width: 1.5px;
        border-radius: 15%;
        margin:5px 0;
    }
    .btnActive, .btnActive:hover {
        background-color: rgba(235, 119, 12, 1)!important;
        color: rgba(0, 45, 75, 1)!important;
        border:none!important;
    }
    .btnActive:hover{
        font-weight: 600;
    }
    #message {
        position: fixed;
        top: 5px;
        left: 5px;
        width: 16.66%;
    }
    #inner-message {
        margin: 0 auto;
    }
</style>
<div id="spinner" class=".container-fluid mx-auto my-auto text-center" style="height:84%!important;display:block;">
    <img src="/img/spinner.gif" alt="loading" style="height: 150px;margin-top: 158px;">
</div>
<main id="main" role="main" class=".container-fluid w-100" style="height:83%!important;display:none">
    <div class="row h-100" style="margin-right: 0;">
        <div class="col-2 h-100" style="padding-right: 0;">
            <div class=".container-fluid text-center h-100" style="background-color: rgba(0, 45, 75, 1);padding-bottom:75px;overflow-y:auto">
               <div id="configurar-btns" class="container" style="width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                   <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">CONFIGURAR</p>
                   <button class="" id="acessoBtn">Acesso</button>
                   <button id="participantesBtn">Participantes</button>
               </div>
                <div id="visualizar-btns" class="container" style="margin-top:15px;width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                    <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">VISUALIZAR</p>
                    <button id="resultadosBtn">Resultados</button>
                </div>
                <div id="indicadores-btns" class="container" style="margin-top:15px;width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                    <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">INDICADORES</p>
                        <button id="indicadorBtn">Informar</button>
                </div>
                <div id="acessoGSIntra-btns" class="container" style="margin-top:15px;width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                    <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">ACESSO GSINTRA</p>
                        <button id="acessoGSBtn">Upload info</button>
                </div>
                <div id="quiz-btns" class="container" style="margin-top:15px;width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                    <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">QUIZ</p>
                        <button id="upQuizBtn">Upload info</button>
                </div>
                <div id="gerar-btns" class="container" style="margin-top:15px;width:92%;padding:0 5px;padding-top:10px;border:rgba(235, 119, 12, 1) 1px solid">
                    <p class="h6" style="padding:0;border:0;color:rgb(235, 119, 12);">PLANILHA</p>
                    <% data.forEach(r => { %>
                        <button id="<%= r.periodo %>"><%= r.periodo %></button>
                    <% }) %>
                </div>
            </div>
        </div>
        <div class="col-10 h-100" style="padding:3px 3px 3px 4px;    padding-bottom: 38px!important">
            <div id="main-content" class=".container-fluid h-100 w-100 text-center my-auto d-table" style="border:rgba(65, 105, 225, 0.534) 14px solid">
              <!--CONTENT GOES HERE-->
            </div>
        </div>
    </div>
    <div class=".container-fluid" style="margin-left: 15vh;flex:2">
    </div>
</main>
<%- include('partials/footer.ejs', {info: info}) %>
<%- include('metaData/commonScripts.ejs') %>
<!--CUSTOMED-->
<script src="/js/rootPage/appCtrl.js"></script>
<script src="/js/rootPage/dataCtrl.js"></script>
<script src="/js/rootPage/uiCtrl.js"></script>
<script src="/js/rootPage/main.js"></script>
<%- include('metaData/end.ejs') %>
